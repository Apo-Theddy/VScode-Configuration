{
    "editor.tokenColorCustomizations": {
        "[apotheme]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "punctuation.definition.comment",
                        "comment.block",
                        "comment.line",
                        "comment.block.documentation",
                        "comment.line.double-slash",
                        "entity.other.attribute-name",
                        "entity.other.attribute-name.html",
                        "markup.italic",
                        "entity.other.attribute-name.id",
                        "entity.other.attribute-name.class.css",
                        "entity.other.attribute-name.tag.jade",
                        "entity.other.attribute-name.class.jade",
                        "entity.other.attribute-name.id.jade",
                        "keyword.control",
                        "storage",
                        "emphasis",
                        "variable.language"
                    ],
                    "settings": {
                        "fontStyle": "italic"
                    }
                }
            ]
        }
    },
    "workbench.iconTheme": "gruvbox-material-icon-theme",
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.editor.showIcons": true,
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "editor.fontFamily": "Maple Mono",
    "workbench.startupEditor": "none",
    "editor.minimap.autohide": true,
    "workbench.editor.wrapTabs": true,
    "editor.wordWrap": "on",
    "editor.guides.indentation": false,
    "editor.formatOnSave": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "underline",
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": true,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.tabCloseButton": "off",
    "workbench.tree.renderIndentGuides": "none",
    "editor.formatOnPaste": true,
    "diffEditor.wordWrap": "on",
    "editor.wrappingIndent": "deepIndent",
    "editor.renderWhitespace": "none",
    "workbench.tree.expandMode": "singleClick",
    "workbench.tree.indent": 8,
    "editor.glyphMargin": false,
    "editor.folding": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.cursorBlinking": "phase",
    "editor.renderLineHighlight": "none",
    "editor.lineNumbers": "off",
    "editor.showFoldingControls": "mouseover",
    "workbench.layoutControl.type": "menu",
    "workbench.layoutControl.enabled": false,
    "terminal.integrated.fontFamily": "Maple Mono",
    "window.title": "",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "material-icon-theme.activeIconPack": "nest",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "redhat.telemetry.enabled": true,
    "workbench.productIconTheme": "fluent-icons",
    "dart.flutterSdkPath": "C:\\development\\flutter",
    "workbench.colorTheme": "Maple Dark",
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.bracketPairColorization.enabled": false,
    "git.openRepositoryInParentFolders": "always",
    "window.menuBarVisibility": "compact",
    "workbench.activityBar.location": "top",
    "window.zoomLevel": 1,
    "liveServer.settings.donotShowInfoMsg": true,
    "dart.debugExternalPackageLibraries": true,
    "dart.debugSdkLibraries": false,
    "git.confirmSync": false,
}
